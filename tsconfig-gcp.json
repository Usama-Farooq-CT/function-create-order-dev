{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": false,
    "outDir": "dist",
    "skipLibCheck": true,
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["**/*.ts",
    "src/schema.graphql"
  ],
  "exclude": [
    "node_modules",
    "src/main.ts",
    "bindings.d.ts",
    "src/kv-cache.ts",
    "src/handlers/*.ts",
    "src/utils/cors.ts"
  ]
}